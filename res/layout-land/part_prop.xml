<?xml version="1.0" encoding="utf-8"?>
<devedroid.opensurveyor.PropertyWindow xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/props"
	android:layout_width="match_parent"
	android:layout_height="match_parent" >
	<Button
		android:id="@+id/btPropsClose"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_alignParentTop="true"
		android:layout_centerHorizontal="true"
		android:paddingLeft="40dp"
		android:paddingRight="40dp"
		android:text="OK" />
	
	<ListView
		android:id="@+id/prop_list"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_alignParentLeft="true"
		android:layout_alignParentRight="true"
		android:layout_below="@id/btPropsClose" >

	</ListView>



</devedroid.opensurveyor.PropertyWindow>